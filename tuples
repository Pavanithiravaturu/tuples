#Add an item

a=(4,6,2,8,3,1)
print(a)
a=a+(9,)
print(a)
a=a[:5]+(15,20,25)+a[:5]
print(a)
b=list(a)
b.append(30)
a=tuple(b)
print(a)

#Length of a tuple

t=tuple(input())
print(t)
print(len(t))

#Reverse a Tuple

t=tuple(input())
y=tuple(reversed(t))
print(y)

#Repeated element in a tuple

n=int(input())
a=2,4,5,6,2,3,4,4,7
print(a)
temp=a.count(n)
print(temp)
